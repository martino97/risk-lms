<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="cooplms" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\inetpub\wwwroot\COOPLMS\env\Scripts\python.exe|C:\inetpub\wwwroot\COOPLMS\env\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
            <add name="ongc" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\inetpub\wwwroot\COOPLMS\env\Scripts\python.exe|C:\inetpub\wwwroot\COOPLMS\env\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
            <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\inetpub\wwwroot\COOPLMS\env\Scripts\python.exe|C:\inetpub\wwwroot\COOPLMS\env\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" />
        </handlers>
		<staticContent>
            <!-- Remove conflicting mappings -->
            <remove fileExtension=".woff" />
            <remove fileExtension=".woff2" />
            <remove fileExtension=".json" />
            <remove fileExtension=".mp4" />
            <remove fileExtension=".webm" />

            <!-- MIME types -->
            <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
            <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
            <mimeMap fileExtension=".json" mimeType="application/json" />
            <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
            <mimeMap fileExtension=".webm" mimeType="video/webm" />
        </staticContent>
		<security>
			<requestFiltering allowDoubleEscaping="true" />
		</security>
        <httpRedirect enabled="false" destination="" />
    </system.webServer>
    
    <appSettings>
        <!-- Django Application Settings -->
        <!-- IMPORTANT: use the project's WSGI entrypoint so migrate-on-start runs -->
        <add key="WSGI_HANDLER" value="risk_lms.wsgi.application" />
        <add key="PYTHONPATH" value="C:\inetpub\wwwroot\risk_lms" />
        <add key="DJANGO_SETTINGS_MODULE" value="risk_lms.settings" />
	</appSettings>
</configuration>
